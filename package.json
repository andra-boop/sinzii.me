{
  "name": "myblog-template",
  "version": "1.0.0",
  "description": "MyBlog Template",
  "main": "index.js",
  "scripts": {
    "dev:serve": "npx gulp -f gulpfile.ts --series dev serve",
    "prod:build": "npx gulp -f gulpfile.ts --series prod build",
    "dev:clean:build": "npx gulp -f gulpfile.ts --series dev clean",
    "prod:clean:build": "npx gulp -f gulpfile.ts --series prod clean",
    "dev:clean:posts": "npx gulp -f gulpfile.ts --series dev cleanPosts",
    "prod:clean:posts": "npx gulp -f gulpfile.ts --series prod cleanPosts",
    "dev:sample": "npx gulp -f gulpfile.ts --series dev generateSamplePosts",
    "prod:sample": "npx gulp -f gulpfile.ts --series prod generateSamplePosts",
    "test": "npx mocha --require ts-node/register \"./test/**/*.test.@(js|ts)\""
  },
  "author": "@sinzii",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/debug": "^4.1.5",
    "@types/faker": "^5.5.5",
    "@types/gulp": "^4.0.8",
    "@types/lodash": "^4.14.170",
    "@types/mocha": "^8.2.2",
    "@types/moment": "^2.13.0",
    "@types/sinon": "^10.0.0",
    "browser-sync": "^2.26.14",
    "chai": "*",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-sass": "^4.1.0",
    "minimist": "^1.2.5",
    "mocha": "^8.4.0",
    "sinon": "^10.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "debug": "^4.3.1",
    "faker": "^5.5.3",
    "glob": "^7.1.7",
    "highlight.js": "^10.7.2",
    "lodash": "^4.17.21",
    "marked": "^2.0.3",
    "moment": "^2.29.1",
    "pug": "^3.0.2",
    "through2": "^4.0.2",
    "vinyl": "^2.2.1"
  }
}
